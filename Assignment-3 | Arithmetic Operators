        //Question 5
        int inputSeconds, hours, minutes, seconds;

        inputSeconds = 3695;
        hours =  (inputSeconds / 3600)%60;
        minutes = (inputSeconds / 60)%60 ;
        seconds = inputSeconds % 60;
        System.out.println(hours + ":" + minutes + ":"+seconds);
        System.out.println();

        //Question 6
