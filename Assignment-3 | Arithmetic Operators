        Totalcaffeine = 500;
        gram = Totalcaffeine/1000;
        totalDrink = 10 /gram;

        System.out.println("Number of milligrams in drink: "+(int)Totalcaffeine);
        System.out.println("It would take about "+ (int)totalDrink+ " drinks for a lethal overdose");
        System.out.println();

